{
  "name": "aik",
  "version": "0.10.0",
  "description": "Frontend Playground",
  "bin": "cli.js",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "cli.js"
  ],
  "keywords": [
    "node",
    "frontend",
    "repl",
    "react",
    "babel",
    "webpack",
    "playground",
    "experiments",
    "boostrap",
    "boilerplate",
    "postcss",
    "cssmodules"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "audit": "nsp check package",
    "build": "babel -d ./ ./src",
    "check": "npm outdated --depth 0",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html npm test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "github-release": "conventional-github-releaser -p angular",
    "lint": "eslint src test",
    "test": "ava test/*.js",
    "validate": "npm run lint && npm test",
    "prepare": "npm run build && npm run validate && npm run check",
    "pp": "npm run prepare",
    "major": "pmm major",
    "minor": "pmm minor",
    "patch": "pmm patch"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "pre-commit": [
    "validate"
  ],
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chalk": "^1.1.3",
    "css-loader": "^0.25.0",
    "eslint": "3.6.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "meow": "^3.7.0",
    "ngrok": "^2.2.2",
    "npm-install-webpack-plugin": "^4.0.4",
    "opn": "^4.0.2",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "update-notifier": "^1.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-register": "^6.14.0",
    "conventional-github-releaser": "^1.1.3",
    "coveralls": "^2.11.13",
    "cz-conventional-changelog": "^1.2.0",
    "nsp": "^2.6.1",
    "nyc": "^8.3.0",
    "pmm": "^1.3.0",
    "pre-commit": "^1.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
