{
  "name": "aik",
  "version": "0.16.2",
  "description": "Frontend Playground",
  "bin": "cli.js",
  "main": "lib/index.js",
  "files": ["cli.js", "lib/", "template/"],
  "keywords": [
    "node",
    "frontend",
    "repl",
    "react",
    "babel",
    "webpack",
    "playground",
    "experiments",
    "boostrap",
    "boilerplate",
    "postcss"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "babel -d ./lib ./src",
    "ci:github-release": "conventional-github-releaser -p angular",
    "ci:validate": "eslint src && flow",
    "ci:coverage": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "lint:flow": "flow",
    "lint:staged": "lint-staged",
    "prepare": "npm run build && npm run ci:validate",
    "release:major": "pmm major",
    "release:minor": "pmm minor",
    "release:patch": "pmm patch",
    "pretest:heavy": "./tests/update-examples.sh",
    "test:heavy": "./tests/heavy/run.sh",
    "pretest:visual": "./tests/update-examples.sh",
    "test:visual": "node ./tests/visual-regression/run.js",
    "test:visual:update": "node ./tests/visual-regression/run.js update",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "pre-commit": ["lint:flow", "lint:staged", "test"],
  "lint-staged": {
    "*.js": ["eslint", "prettier --write --print-width 120", "git add"]
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-traverse": "6.25.0",
    "babylon": "6.17.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.28.4",
    "eslint": "4.2.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "gzip-size": "3.0.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.29.0",
    "insight": "0.8.4",
    "json-loader": "0.5.4",
    "meow": "3.7.0",
    "ngrok": "2.2.15",
    "npm-install-webpack2-plugin": "5.0.1",
    "opn": "5.1.0",
    "postcss-loader": "2.0.6",
    "postcss-partial-import": "4.1.0",
    "precss": "2.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "resolve": "1.3.3",
    "style-loader": "0.18.2",
    "webpack": "3.3.0",
    "webpack-dev-server": "2.5.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-polyfill": "6.23.0",
    "conventional-github-releaser": "1.1.12",
    "coveralls": "2.13.1",
    "cz-conventional-changelog": "2.0.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-flowtype": "2.35.0",
    "flow-bin": "0.50.0",
    "gemini": "5.0.0-alpha.3",
    "html-reporter": "0.2.0",
    "jest": "20.0.4",
    "lint-staged": "4.0.2",
    "pmm": "1.3.1",
    "pre-commit": "1.2.2",
    "prettier": "1.5.3",
    "proq": "1.0.0",
    "rimraf": "2.6.1",
    "strip-ansi": "4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "roots": ["./src"],
    "collectCoverageFrom": ["src/**/*.js"]
  }
}
