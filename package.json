{
  "name": "aik",
  "version": "0.12.1-wip",
  "description": "Frontend Playground",
  "bin": "cli.js",
  "main": "lib/index.js",
  "files": [
    "cli.js",
    "lib/",
    "template/"
  ],
  "keywords": [
    "node",
    "frontend",
    "repl",
    "react",
    "babel",
    "webpack",
    "playground",
    "experiments",
    "boostrap",
    "boilerplate",
    "postcss",
    "cssmodules"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "audit": "nsp check package",
    "prebuild": "rimraf ./lib",
    "build": "babel -d ./lib ./src",
    "check": "npm outdated --depth 0",
    "github-release": "conventional-github-releaser -p angular",
    "lint": "eslint src",
    "validate": "npm run lint && flow",
    "prepare": "npm run build && npm run validate && npm run check",
    "pp": "npm run prepare",
    "major": "pmm major",
    "minor": "pmm minor",
    "patch": "pmm patch"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "pre-commit": [
    "validate"
  ],
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.7",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.0",
    "detect-port": "^1.0.5",
    "eslint": "3.11.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gzip-size": "^3.0.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "insight": "^0.8.4",
    "json-loader": "^0.5.4",
    "meow": "^3.7.0",
    "ngrok": "^2.2.3",
    "npm-install-webpack-plugin": "github:d4rkr00t/npm-install-webpack-plugin#check-exact",
    "opn": "^4.0.2",
    "output-file": "^1.1.1",
    "postcss-loader": "^1.1.1",
    "postcss-partial-import": "^2.1.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "conventional-github-releaser": "^1.1.3",
    "cz-conventional-changelog": "^1.2.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "flow-bin": "^0.36.0",
    "nsp": "^2.6.2",
    "pmm": "^1.3.1",
    "pre-commit": "^1.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
