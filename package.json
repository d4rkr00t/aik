{
  "name": "aik",
  "version": "0.15.0",
  "description": "Frontend Playground",
  "bin": "cli.js",
  "main": "lib/index.js",
  "files": [
    "cli.js",
    "lib/",
    "template/"
  ],
  "keywords": [
    "node",
    "frontend",
    "repl",
    "react",
    "babel",
    "webpack",
    "playground",
    "experiments",
    "boostrap",
    "boilerplate",
    "postcss",
    "cssmodules"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "babel -d ./lib ./src",
    "ci:github-release": "conventional-github-releaser -p angular",
    "ci:validate": "eslint src && flow",
    "lint:flow": "flow",
    "lint:staged": "lint-staged",
    "prepare": "npm run build && npm run ci:validate",
    "release:major": "pmm major",
    "release:minor": "pmm minor",
    "release:patch": "pmm patch",
    "test": "./tests/run.sh"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "pre-commit": [
    "lint:flow",
    "lint:staged"
  ],
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.1",
    "babel-loader": "6.3.2",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.0",
    "gzip-size": "3.0.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "insight": "0.8.4",
    "json-loader": "0.5.4",
    "meow": "3.7.0",
    "ngrok": "2.2.7",
    "npm-install-webpack-plugin": "github:webpack-contrib/npm-install-webpack-plugin#b21bd32dce7470d5169c0964ac4be15b2899a005",
    "opn": "github:sindresorhus/opn#b792b3bc3ad8a1f1090bffc5ef8f7e01f4a24697",
    "postcss-loader": "1.3.3",
    "postcss-partial-import": "3.1.1",
    "precss": "1.4.0",
    "react-hot-loader": "1.3.1",
    "resolve": "1.2.0",
    "style-loader": "0.16.1",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015-node4": "2.1.1",
    "conventional-github-releaser": "1.1.3",
    "cz-conventional-changelog": "2.0.0",
    "eslint-plugin-flowtype": "2.32.1",
    "flow-bin": "0.45.0",
    "lint-staged": "^3.4.0",
    "pmm": "1.3.1",
    "pre-commit": "1.2.2",
    "prettier": "1.2.2",
    "rimraf": "2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
