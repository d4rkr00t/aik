{
  "name": "aik",
  "version": "0.14.0",
  "description": "Frontend Playground",
  "bin": "cli.js",
  "main": "lib/index.js",
  "files": [
    "cli.js",
    "lib/",
    "template/"
  ],
  "keywords": [
    "node",
    "frontend",
    "repl",
    "react",
    "babel",
    "webpack",
    "playground",
    "experiments",
    "boostrap",
    "boilerplate",
    "postcss",
    "cssmodules"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "audit": "nsp check package",
    "prebuild": "rimraf ./lib",
    "build": "babel -d ./lib ./src",
    "github-release": "conventional-github-releaser -p angular",
    "lint": "eslint src",
    "validate": "npm run lint && flow",
    "prepare": "npm run build && npm run validate",
    "pp": "npm run prepare",
    "major": "pmm major",
    "minor": "pmm minor",
    "patch": "pmm patch",
    "test": "./tests/run.sh"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "pre-commit": [
    "validate"
  ],
  "dependencies": {
    "autoprefixer": "6.7.6",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.26.4",
    "detect-port": "1.1.0",
    "eslint": "3.16.1",
    "eslint-loader": "1.6.3",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.1",
    "fs-extra": "2.1.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.1",
    "insight": "0.8.4",
    "json-loader": "0.5.4",
    "meow": "3.7.0",
    "ngrok": "2.2.6",
    "npm-install-webpack-plugin": "github:webpack-contrib/npm-install-webpack-plugin#b21bd32dce7470d5169c0964ac4be15b2899a005",
    "opn": "github:sindresorhus/opn#b792b3bc3ad8a1f1090bffc5ef8f7e01f4a24697",
    "postcss-loader": "1.3.2",
    "postcss-partial-import": "3.1.1",
    "precss": "1.4.0",
    "react-hot-loader": "1.3.1",
    "resolve": "^1.2.0",
    "style-loader": "0.14.0",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.3"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015-node4": "2.1.1",
    "conventional-github-releaser": "1.1.3",
    "cz-conventional-changelog": "2.0.0",
    "eslint-plugin-flowtype": "2.30.0",
    "flow-bin": "0.40.0",
    "nsp": "2.6.2",
    "pmm": "1.3.1",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
