{
  "name": "aik",
  "version": "0.17.0-dev",
  "description": "Frontend Playground",
  "bin": "cli.js",
  "main": "lib/index.js",
  "files": [
    "cli.js",
    "lib/",
    "template/"
  ],
  "keywords": [
    "node",
    "frontend",
    "repl",
    "react",
    "babel",
    "webpack",
    "playground",
    "experiments",
    "boostrap",
    "boilerplate",
    "postcss"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "babel -d ./lib ./src",
    "ci:github-release": "conventional-github-releaser -p angular",
    "ci:validate": "eslint src && flow",
    "ci:coverage": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "lint:flow": "flow",
    "lint:staged": "lint-staged",
    "pmm:prepare": "npm run build && npm run ci:validate",
    "release:major": "pmm major",
    "release:minor": "pmm minor",
    "release:patch": "pmm patch",
    "pretest:heavy": "./tests/update-examples.sh",
    "test:heavy": "./tests/heavy/run.sh",
    "pretest:visual": "./tests/update-examples.sh",
    "test:visual": "jest -c tests/visual-regression/jest.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "https://github.com/d4rkr00t/aik"
  },
  "pre-commit": [
    "lint:flow",
    "lint:staged",
    "test"
  ],
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write --print-width 120",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "8.1.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-traverse": "6.26.0",
    "babylon": "6.18.0",
    "chalk": "2.3.2",
    "connect-history-api-fallback": "1.5.0",
    "css-loader": "0.28.10",
    "eslint": "4.18.2",
    "eslint-loader": "2.0.0",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "gzip-size": "4.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.0.4",
    "insight": "0.10.0",
    "meow": "4.0.0",
    "ngrok": "2.3.0",
    "opn": "5.2.0",
    "postcss-loader": "2.1.1",
    "postcss-partial-import": "4.1.0",
    "precss": "3.1.2",
    "react-hot-loader": "4.0.0",
    "resolve": "1.5.0",
    "style-loader": "0.20.2",
    "webpack": "4.0.1",
    "webpack-dev-server": "3.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-jest": "22.4.1",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "conventional-github-releaser": "2.0.0",
    "coveralls": "3.0.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.46.1",
    "flow-bin": "0.66.0",
    "jest": "22.4.2",
    "jest-image-snapshot": "^2.3.0",
    "lint-staged": "7.0.0",
    "pmm": "2.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.11.1",
    "proq": "1.0.0",
    "puppeteer": "^1.1.1",
    "rimraf": "2.6.2",
    "strip-ansi": "4.0.0",
    "webpack-cli": "^2.0.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "roots": [
      "./src"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
